// Ribozyme (TRUE is with; FALSE is without); Default is TRUE.
boolean riboz = true;

Device Cello_gate();
Rule r1(on Cello_gate: ALL_FORWARD);

for(num i=1; i<=N; i=i+1) {
  if(riboz == false) {
  	Cello_gate = Cello_gate + RBS + CDS + Terminator;
  } else {
   Cello_gate = Cello_gate + RBS + CDS + Terminator;
  }
  Ribozyme${"ri"+i};
  RBS${"rbs"+i};
  CDS${"g"+i};
  Terminator${"t"+i};
}

 lod = product(Cello_gate);
 println("The total number of devices: " + SIZEOF(lod));
